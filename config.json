  "manager": {
    "sleepTime": 3,
    "waitForPosition": true,
    "cleanshutdPipe": "/tmp/cleanshutd"
  },
  "database": {
    "host": "localhost",
    "database": "pwnpi",
    "user": "postgres",
    "password": "postgres"
  }
