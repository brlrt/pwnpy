{
  "Manager": {
    "sleepTime": 3,
    "waitForPosition": true,
    "cleanshutdEnable": false,
    "cleanshutdPipe": "/tmp/cleanshutd",

    "Database": {
      "host": "localhost",
      "database": "pwnpi",
      "user": "postgres",
      "password": "postgres"
    },

    "GPS": {
      "enable": true,
      "sleepTime": 1,
      "reCounterMax": 5
    },

    "modules": {
      "UI": {
        "enable": true
      },
      "WiFi": {
        "autoInterface": false,
        "interface": "wlan0mon",
        "promiscuous": true,
        "enable": true,
        "reCounterMax": 5,
        "packetsPerChannel": 10,
        "timeoutPerChannel": 3,
        "channels": [1],
        "hcx_interface": "wlan1mon",
        "hcx_enable": true,
        "hcx_output": "hcxdumps/",
        "hcx_time": 10
      },
      "HCX": {
        "interface": "wlan1mon",
        "enable": true,
        "output": "hcxdumps/"
      },
      "Bluetooth": {
        "interface": "hci0",
        "onlyLE": false,
        "onlyClassic": false,
        "enable": true,
        "classicScanTime": 4,
        "leScanTime": 2,
        "reCounterMax": 5
      },
      "Server": {
        "enable": true,
        "host": "0.0.0.0",
        "port": 1337,
        "threaded": true,
        "defaultPosition": [49.460983, 11.061859]
      }
    }
  }
}
